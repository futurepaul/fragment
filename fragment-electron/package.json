{
  "name": "svelte-app",
  "version": "1.0.0",
  "main": "electron_main.js",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^9.1.0",
    "electron-build-env": "^0.2.0",
    "electron-packager": "^13.1.1",
    "electron-rebuild": "^1.8.4",
    "mini-css-extract-plugin": "^0.6.0",
    "node-loader": "^0.6.0",
    "serve": "^11.0.0",
    "style-loader": "^0.23.1",
    "svelte": "^3.4.2",
    "svelte-loader": "2.13.3",
    "webpack": "^4.32.1",
